[
    {
        "field_type": "input",
        "type": "text",
        "id": "assignment_name",
        "name": "assignment_name",
        "label": "Assignment Name *",
        "required": "Please enter assignment name"
    },

    {
        "field_type": "datepicker",
        "id": "start_date",
        "name": "start_date",
        "label": "Start Date *",
        "minDate":true,
        "required": "Select start date",
        "validationMsg": "Select start date"
    },

    {
        "field_type": "datepicker",
        "id": "end_date",
        "name": "end_date",
        "label": "End Date *",
        "minDate":true,
        "required": "Select end date",
        "validationMsg": "Select end date"
    },
    {
        "field_type": "toggle",
        "id": "assignment_setting",
        "name": "assignment_setting",
        "label": "Assignment settings *",
        "options": [
            "yes",
            "no"
        ]
    },
    {
        "field_type": "button",
        "type": "submit",
        "label": "Create Assignment"
    }
]